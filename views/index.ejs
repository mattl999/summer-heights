<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Summer Heights 1</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
  <link rel="stylesheet" href="/stylesheets/style.css">
  <link rel="stylesheet" href="/stylesheets/tour.css">
  <link rel="stylesheet" href="/stylesheets/shop.css">
  <link rel="stylesheet" href="/stylesheets/contact.css">
  <script defer src="/javascripts/script.js"></script>
</head>

<body>
  <div class="section nav-section nav-font ">
    <nav class="navbar navbar-dark navbar-expand-lg  nav-tweak mt-4">
      <div class="container-fluid px-4 mobile-margin">
        <div class="col-lg-4 col-12">
          <a href="https://www.youtube.com/watch?v=pKZeWHX3Na8&ab_channel=SummerHeights" class="social-link" target="_blank">
            <img src="/img/yt-icon.svg" alt="youtube" class="icon">
          </a>
          <a href="https://open.spotify.com/track/42FjlGrFOZ958Dj0ctpqkr?si=4f844526d31842f1&nd=1" class="social-link" target="_blank">
            <img src="/img/spotify-icon.svg" alt="spotify" class="icon">
          </a>
          <a href="https://www.tiktok.com/@summerheightsband" class="social-link" target="_blank">

            <img src="/img/tiktok-icon.svg" alt="tiktok" class="icon">
          </a>
          <a href="https://www.instagram.com/summerheightsband/" class="social-link" target="_blank">

            <img src="/img/insta-icon.svg" alt="instagram" class="icon">
          </a>

        </div>
        <div class="col-lg-4 d-flex justify-content-center align-items-end brand-div" style="height:100%;">
          <a class="navbar-brand nav-font p-0  brand-tweak d-flex align-items-end" href="#">Summer Heights</a>
        </div>
        <div class="col-lg-4 d-flex ">
          <div class="navbar-toggler w-nav-button menu-button" type="button" role="button" data-bs-toggle="collapse" data-bs-target="#navbarToggler" aria-controls="navbarToggler" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon hamburger"></span>
          </div>
          <div class="collapse navbar-collapse " role="navigation" aria-expanded="false" id="navbarToggler">
            <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
              <li class="nav-item ">
                <a class="nav-link py-0 nav-items nav-link-mod" href="#Tour"> Tour</a>
              </li>
              <li class="nav-item ">
                <a class="nav-link  py-0 nav-items nav-link-mod" href="#Shop">Shop</a>
              </li>
              <li class="nav-item ">
                <a class="nav-link py-0 nav-items nav-link-mod" href="#Contact">Contact</a>
              </li>
            </ul>

          </div>
        </div>
      </div>
    </nav>
    <div class="container d-flex justify-content-start align-items-center feature-wrapper">
      <div class="feature-container">
      </div>
    </div>
  </div>
  <div class="tour-section py-2" id="Tour">
    <div class="container py-4 tour-container-tweak" style="width: 69%">
      <h1 class="mb-5 section-header">TOUR</h1>
      <% if(tourDates.length){ %>
      <% tourDates.forEach((el) => { %>
      <div class="row tour-date-container py-4  my-4">
        <div class="col-md-3 col-12 d-flex justify-content-center align-content-center date-div p-2">
          <div class="date"><%= el.month %> <%= el.day %>, <%= el.year %></div>
          <div class="time"><%= el.weekday %> <%= el.time %> </div>
        </div>
        <div class="col-md-5 location-div">
          <div class="city">
            <%= el.city %>, <%= el.province %>
          </div>
          <div class="venue">
            <%= el.venue %>
          </div>
        </div>
        <div class="col-md-4 col-12 ticket-div">
          <a class="ticket-link p-2 px-4 mt-2" href="<%= el.ticketLink %>" target="_blank">buy tickets</a>
        </div>
      </div>
      <% }) %>
      <% } else { %>
      <div class="row not-touring-container py-4  my-4 mt-5">
        Not currently touring...
      </div>
      <% } %>
    </div>
  </div>
  <div class="shop-section py-2" id="Shop">
    <div class="container py-5" style="width: 75%">
      <h1 class="mb-5">SHOP</h1>
      <div class="container-fluid overflow-hidden">
